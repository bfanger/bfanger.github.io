{
  "name": "bfanger.nl",
  "private": true,
  "scripts": {
    "dev": "sapper dev --no-hot",
    "lint": "eslint --max-warnings=0 src",
    "test": "npm run lint",
    "deploy": "sapper export --no-subfolders  && push-dir --dir __sapper__/export --branch=master"
  },
  "prettier": {
    "semi": false
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "corejs": 3,
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "browserlist": [
    "defaults"
  ],
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^1.0.0-next.11",
    "sirv": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "commonmark": "^0.29.1",
    "core-js": "^3.6.5",
    "eslint": "^7.3.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "gray-matter": "^4.0.2",
    "image-size": "^0.8.3",
    "install": "^0.13.0",
    "node-sass": "^4.14.1",
    "or": "^0.2.0",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.0.5",
    "push-dir": "^0.4.1",
    "sapper": "git+https://git@github.com/bfanger/sapper.git#feature/no-subfolders",
    "sass": "^1.26.9",
    "svelte": "^3.23.2",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.9.10",
    "three": "^0.118.3",
    "webpack": "^4.43.0"
  }
}
