{
  "name": "bfanger.nl",
  "version": "3.0.0",
  "description": "",
  "homepage": "http://bfanger.nl/",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run dev:jekyll' 'npm run dev:webpack'",
    "dev:jekyll": "bundle exec jekyll serve --source ./jekyll --host 0.0.0.0 --incremental --drafts",
    "dev:webpack": "webpack-dev-server -d --inline --hot --history-api-fallback --content-base _site --host 0.0.0.0 --color",
    "dev-fast-recompile": "webpack-dev-server -d --history-api-fallback --content-base _site --host 0.0.0.0",
    "deploy": "concurrently 'npm run deploy:webpack' 'npm run deploy:jekyll'",
    "deploy:jekyll": "bundle exec jekyll build --source ./jekyll",
    "deploy:webpack": "webpack -p --progress"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-image": "^2.8.0",
    "gulp-responsive": "^2.7.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "rxjs": "^5.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.2",
    "vue-rx": "^3.1.0",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
