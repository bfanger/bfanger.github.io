{
  "name": "bfanger.nl",
  "version": "3.0.0",
  "description": "",
  "homepage": "http://bfanger.nl/",
  "scripts": {
    "start": "concurrently --kill-others 'npm run dev-jekyll' 'npm run dev-hot-reloading'",
    "dev-jekyll": "bundle exec jekyll serve --source ./jekyll --host 0.0.0.0 --incremental --drafts",
    "dev-hot-reloading": "webpack-dev-server -d --inline --hot --history-api-fallback --content-base _site --host 0.0.0.0 --color",
    "dev-fast-recompile": "webpack-dev-server -d --history-api-fallback --content-base _site --host 0.0.0.0",
    "deploy-jekyll": "bundle exec jekyll build --source ./jekyll",
    "deploy-webpack": "webpack -p --progress",
    "deploy": "concurrently 'npm run deploy-webpack' 'npm run deploy-jekyll'"
  },
  "dependencies": {
    "concurrently": "^3.1.0",
    "rxjs": "^5.0.0-rc.1",
    "vue": "^2.0.3",
    "vue-rx": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-image": "^2.7.3",
    "gulp-responsive": "^2.7.0",
    "node-sass": "^3.11.1",
    "postcss-loader": "^1.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
