{
  "name": "bfanger.nl",
  "version": "3.0.0",
  "description": "",
  "homepage": "http://bfanger.nl/",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run dev:jekyll' 'npm run dev:webpack'",
    "dev:jekyll": "bundle exec jekyll serve --source ./jekyll --host 0.0.0.0 --incremental --drafts",
    "dev:webpack": "webpack-dev-server -d --inline --hot --history-api-fallback --content-base _site --host 0.0.0.0 --color",
    "dev-fast-recompile": "webpack-dev-server -d --history-api-fallback --content-base _site --host 0.0.0.0",
    "deploy": "concurrently 'npm run deploy:webpack' 'npm run deploy:jekyll'",
    "deploy:jekyll": "bundle exec jekyll build --source ./jekyll",
    "deploy:webpack": "webpack -p --progress"
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-image": "^4.0.0",
    "gulp-responsive": "^2.8.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.7",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue": "^2.5.0",
    "vue-loader": "^13.3.0",
    "vue-rx": "^4.0.1",
    "vue-template-compiler": "^2.5.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  }
}
